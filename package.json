{
  "name": "@atzentis/cli",
  "version": "0.1.0",
  "description": "Autonomous development CLI combining Atzentis Agents with Ralphy patterns",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "atzentis": "./dist/cli.js",
    "atzentis-cli": "./dist/cli.js"
  },
  "scripts": {
    "dev": "bun run src/cli.ts",
    "build": "bun build src/cli.ts --outdir dist --target bun --minify",
    "build:node": "bun build src/cli.ts --outdir dist --target node",
    "check": "bunx biome check .",
    "check:fix": "bunx biome check --write .",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "cli",
    "autonomous",
    "ai",
    "claude",
    "development",
    "orchestration"
  ],
  "author": "Atzentis",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "commander": "^13.0.0",
    "yaml": "^2.7.0",
    "simple-git": "^3.27.0",
    "@octokit/rest": "^21.0.0",
    "picocolors": "^1.1.0",
    "nanospinner": "^1.2.0",
    "zod": "^3.24.0",
    "handlebars": "^4.7.8",
    "minimatch": "^10.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@types/bun": "^1.3.0",
    "typescript": "^5.7.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
